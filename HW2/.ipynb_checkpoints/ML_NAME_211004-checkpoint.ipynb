    "def classify(message, ham_vec, spam_vec):\n",
    "    \n",
    "    p_spam_given_message = 1\n",
    "    p_ham_given_message = 1\n",
    "    \n",
    "    for word in message:\n",
    "        if word not in spam_vec:\n",
    "            p_spam_given_message *= 1\n",
    "        else:\n",
    "            p_spam_given_message *= spam_vec[word]\n",
    "            \n",
    "        if word not in ham_vec: \n",
    "            p_ham_given_message *= 1\n",
    "        else:\n",
    "            p_ham_given_message *= ham_vec[word]\n",
    "            \n",
    "    if p_ham_given_message > p_spam_given_message:\n",
    "        res = 'Ham'\n",
    "    elif p_ham_given_message < p_spam_given_message:\n",
    "        res = 'Spam'\n",
    "    else:\n",
    "        res = 'I dont know'\n",
    "        #print(message, ' I need human help')\n",
    "    return res\n",
