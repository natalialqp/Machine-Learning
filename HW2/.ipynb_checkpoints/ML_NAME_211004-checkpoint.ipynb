    "            return file\n",
    "    else:\n",
    "        test = []\n",
    "        for line in open(nameFile,'r').readlines():\n",
    "            columns = []\n",
    "            for col in line.split('\t'):\n",
    "                columns.append(col)\n",
    "            test.append(columns)\n",
