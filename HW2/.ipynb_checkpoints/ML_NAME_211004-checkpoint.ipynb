    "def create_vocabulary(text):\n",
    "    vocabulary = []\n",
    "    for sms in text:\n",
    "        for word in sms:\n",
    "            vocabulary.append(word)\n",
    "    return vocabulary\n",
    "ham_vec = {}\n",
    "spam_vec = {}\n",
    "\n",
    "def create_dic(text):\n",
    "    for i, line in text.iterrows():\n",
    "        if line['type'] == 'spam':\n",
    "            for word in line['text']:\n",
    "                if word not in spam_vec:\n",
    "                    spam_vec[word] = 1\n",
    "                else:\n",
    "                    spam_vec[word] += 1\n",
    "        if line['type'] == 'ham':\n",
    "            for word in line['text']:\n",
    "                if word not in ham_vec:\n",
    "                    ham_vec[word] = 1\n",
    "                else:\n",
    "                    ham_vec[word] += 1\n",
    "    return ham_vec, spam_vec\n",
